{
  "name": "next-ignite",
  "version": "0.2.5",
  "bin": {
    "ignite": "bin/ignite.js"
  },
  "module": "./dist/esm/index.js",
  "types": "./dist",
  "repository": "hipstersmoothie/next-ignite",
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "scripts": {
    "dev": "yarn ignite dev",
    "build:lib": "ds build",
    "ignite": "./bin/ignite.js",
    "build:docs": "yarn ignite build",
    "release": "auto shipit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist",
    "bin",
    "next.js"
  ],
  "dependencies": {
    "@mapbox/rehype-prism": "^0.4.0",
    "@next/bundle-analyzer": "^9.4.0",
    "@react-hook/passive-layout-effect": "^1.2.0",
    "clsx": "^1.1.0",
    "command-line-application": "^0.9.6",
    "copy-template-dir": "^1.4.0",
    "cosmiconfig": "^6.0.0",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.2",
    "gravatar": "^1.8.0",
    "mdx-prism": "^0.3.1",
    "next": "^9.4.4",
    "next-mdx-enhanced": "^3.0.0",
    "next-prefixed": "^0.0.11",
    "parse-github-url": "^1.0.2",
    "push-dir": "^0.4.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "rehype": "^9.0.1",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^3.0.0",
    "rehype-slug": "^3.0.0",
    "remark-emoji": "^2.0.2",
    "scroll-lock": "^2.1.4",
    "title-case": "^3.0.2"
  },
  "devDependencies": {
    "@auto-it/gh-pages": "^9.39.0",
    "@design-systems/cli": "^1.4.3",
    "@types/gravatar": "^1.8.1",
    "@types/mdx-js__react": "^1.5.1",
    "auto": "^9.39.0",
    "autoprefixer": "^9.7.4",
    "ignite": "link:./",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "tailwindcss": "^1.4.6",
    "tailwindcss-dark-mode": "^1.1.4"
  },
  "ignite": {
    "repo": "hipstersmoothie/next-mdx-dynamic-docs",
    "basePath": "/next-ignite"
  },
  "auto": {
    "plugins": [
      "npm",
      [
        "gh-pages",
        {
          "buildCommand": "npm run build:docs",
          "dir": "./docs/out"
        }
      ]
    ]
  }
}
